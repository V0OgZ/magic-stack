{
  "name": "realgame-qa-stealth",
  "version": "1.0.0",
  "description": "QA Testing Suite for REALGAME - Definitely Not Tucker",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:quick": "./run-stealth-tests.sh quick",
    "test:full": "./run-stealth-tests.sh full",
    "test:debug": "./run-stealth-tests.sh debug",
    "test:ui": "playwright test --ui",
    "report": "playwright show-report reports/html",
    "codegen": "playwright codegen",
    "install:browsers": "playwright install",
    "clean": "rm -rf reports/* test-results/*"
  },
  "keywords": [
    "qa",
    "testing",
    "playwright",
    "not-tucker",
    "stealth"
  ],
